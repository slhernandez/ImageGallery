@import './common'
@import './spacecraft-bgimg'

#notification
  &, & a
    color #fff
  & a
    text-decoration underline
  position relative
  bold()
  text-align center
  font-size font-size-18
  text-shadow 0 1px 0 rgba(0,0,0,.2)
  overflow hidden
  height 0
  background error-color
  &.warning
    background #fe6417
  .inner
    padding 15px 50px
    box-sizing(border-box)
    .ie8 &
      max-width 985px
  .close
    top 50%
    margin-top -5px
    right 7px

span.upgrade-current-site,
span.update-payment-info
  text-decoration underline
  cursor pointer
  &.blue
    color #369

/* Global Header */
#global
  h1
    top 15px
    &.logo,
    a
      width 17px
      background-image url('data:image/png;base64,') 

/* Primary Nav */
#primary
  position absolute
  left 45px
  bottom 18px
  overflow hidden
  ul
    list-style none
    overflow hidden
  li
    display inline-block
    margin-right 30px
    cursor pointer
    a
      display inline-block
      font-weight bold
      single-line(font-size-18)
      color #fff
      text-decoration none
      text-shadow none
      &:hover
        text-decoration none
        text-shadow 2px 2px 0 #000
      &[disabled]
        &,
        &:hover,
        &:active
          color #666
          text-decoration none
          text-shadow none
          cursor default
    &.selected a
      &,
      &:hover
        color #fff
        text-decoration none
        text-shadow none

#dp-controls
  display none
  position absolute
  z-index -1
  right 40px
  bottom 0

#dp-toggle
  min-width 110px
  text-align center
  transition(opacity 250ms ease-in-out)
  &.hidden
    display block
    opacity 0
  &.inactive
    visibility hidden

#header-toggle
  width 41px
  text-indent 100%
  white-space nowrap
  overflow hidden
  padding 0
  background-image url(double-arrows-up-white)
  background-position 50% 50%
  background-repeat no-repeat

#header.closed #header-toggle
  background-image url(double-arrows-down-white)

/* Modal */
#modal-container,
#modal-mask
  width 100%
  height 100%
  position fixed
  top 0
  left 0

#modal-container
  z-index 101
          
#modal-mask
  background black
  opacity(.6)

#modal-content
  position relative 
  z-index 1
  box-sizing(border-box)
  top 10%
  width 82%
  min-width 675px
  max-width 975px
  max-height 80%
  padding 0 20px
  rounded-corners(6px)
  background white
  box-shadow(0 0 6px 2px rgba(51,51,51,.7))
  margin 0 auto 0 auto
  overflow hidden
  clearfix()
  header
    padding-top 20px
    h1
      margin 0 60px 0 0
      padding-bottom 10px
    a
      position absolute
      top 23px
      right 20px
      &[disabled]
        &,
        &:hover
          color #ccc
          text-decoration none
          cursor default
  .modal-content
    padding-bottom 20px
    .image-grid
      float left
      width 100%
    .controls
      overflow hidden // float-clearing for some cases
    form.scrollable
      overflow auto
    form.needsScrolling
      .scrollbox
        overflow auto
    fieldset.fixed-controls
      border-top solid 1px #dedede
      background #fff
    fieldset.next-to-last
      border-bottom none
      padding-bottom 0

#uploadNewAdd
  margin 0 0 20px 0

#modal-buttonbar
  float left
  width 100%
  text-align right 
  margin-top 10px
  padding 20px 0 10px 0
  border-top solid 1px #cdcdcd
  button
    margin 0 0 0 20px
  #modal-close-button
    float left
    margin-left 0

/* Content */
#main h1
  margin-bottom 30px  

/* image library title/details editor */
#image-details-edit
  position absolute
  top -1000px
  margin 0 auto
  width 93.5%
  min-width 680px
  max-width 1085px
  left 50%
  z-index 140
  &.library
    width 82%
    min-width 655px
    max-width 955px
    padding 0 20px
  #use-defaults-fieldset
    display none
  .closeLink
    position absolute
    top 20px
    right 40px
    text-align right
  div.content
    margin 0 20px
    padding 0 20px
    box-sizing(border-box)
    border solid 1px #ccc
    gradient-fill(#f1f1f1, #e9e9e9)
    fieldset
      border 0
    label
      left 20px
      width 18%
    #use-defaults-fieldset
      padding-top 10px
      padding-bottom 0
      label
        left auto
        width auto
  fieldset.controls
    text-align right
    padding-bottom 10px
    span
      text-align right
      button
        margin 0 0 0 10px          


#edit-btn-bottom-mask
  background #f3f3f3
  height 2px
  position absolute
  z-index 1000
  left -9999px
  .ie8 &
    display none // ie8 can't position this thing right to save its life, of course, so screw it


/* generated lists */
.generated-list
  li
    border-top solid 1px #dedede
    padding-top 15px
    position relative
    margin-top 15px
    overflow hidden
    white-space nowrap
    padding-right 30px
    text-overflow ellipsis
    font-size font-size-13
    &:first-child
      margin-top 5px
    .close
      right 0
      top auto
      bottom -2px
      padding 5px
      background-position 50% 50%
    .edit
      position absolute
      right 40px
  &.resources-list
    margin-bottom 0
    li
      margin-top 0
      padding 17px 115px 17px 46px
      background-position 0 11px
      background-repeat no-repeat
      &.pdf
        background-image url(pdf_icon_26x26)
      .close
        bottom auto
        top 50%
        margin-top -8px

.resource-library
  .resources-list
    li
      min-height 16px
      &:first-child
        border-top 0
      .filename
        white-space nowrap
        overflow hidden
        text-overflow ellipsis
      .edit
        position absolute
        right 40px
      .editor
        position absolute
        top 9px
        left 34px
        right 40px
        input
          width 100%
          padding-right 115px
          margin 0
          line-height 36px
          height 36px
        .buttons
          position absolute
          top 6px
          right 4px
          text-align right
        button, 
        .link-button
          height 23px
          line-height 21px
          font-size font-size-11
          padding-left 5px
          padding-right 5px
          margin 0 0 0 10px
      .close
        margin-top -10px
    &.select_mode
      li
        padding-left 77px
        padding-right 0
        background-position 30px 11px 
        input[type=checkbox]
          position absolute
          top 15px
          left 0

fieldset.add-content-section
  text-align left
  p
    margin-bottom 5px
  label
    position static
    text-transform none
    margin 0
    display inline
    width auto
    font-size 100%
  input
    overflow hidden
    padding 0
    margin 0
  strong
    font-family bold-family
    font-size font-size-18
    margin 0 10px
  
/* Gallery */
.asset-library section.content 
  overflow hidden // float-clearing
  h3
    text-align center
    margin 60px 0
    display none
  &.empty
    h3
      display block

/* Item delete button */
button#deleteItem
  position absolute
  right 0
  top 8px
  display none

/* Featured content */
.add-featured-content
  min-height 270px
  .form-replacement
    margin 0

/* Content Overview */
#content-overview
  padding 20px 0
  margin-top 20px
  border-top solid 1px #ccc
  ul
    margin 0
    padding 0
  li
    div.content-section
      gradient-fill(#f1f1f1, #e9e9e9) 
      gradient-rounded-corners(border-radius-size)
      border solid 1px #ccc;
      box-shadow(0px 0px 4px #ccc)
      position relative // for the buttonbars that get nested inside
      margin-bottom 20px
      padding 13px 250px 14px 20px
    .content-overview-buttonbar
      top 50%
      margin-top -15px
      right 10px
  a
    font-family bold-family
    font-size font-size-14
    line-height 20px
    max-width 100%
    display block
    overflow hidden
    white-space nowrap
    text-overflow ellipsis
  ul li ul
    margin-left 40px
 
/* Content Items */
h2.has-help
  position relative
  a.help-link
    position absolute
    top 2px

/* design tool page */
.design_tool
  height 100%
  overflow hidden
  overflow-y hidden
  overflow-x scroll
  background-color #808080
  body
    height 100%
    background-color #808080
  #main
    padding 0
    min-width 0
    width 100%
    background-color #808080
    .inner
      min-width 0
      max-width none
      width 100%
  #main.outer
    min-width 0
  #design_view
    display block
    width 1024px
    height 768px
    margin 0 auto
    border 0
    background #fff
    box-shadow(0 3px 8px 0 #666666)
    .ie8&
      border solid 2px #adadad
      border-top-width 0
    &.tablets
      width 768px
      height 1024px
      .no-touch&
        width 784px
    &.mobile
      width 320px
      height 480px
      .no-touch&
        width 336px
  #design_view_drag_mask
    position absolute
    top 0
    left 0
    right 0
    bottom 0
    width 100%
    background transparent
    .ie8& // IE can't hear the mousemove if it's fully transparent:
      background white
      opacity(.01)

  &.no-touch
    #design_view
      -webkit-transition-property width, height  
      -webkit-transition-duration 500ms, 500ms
      -moz-transition-property width, height  
      -moz-transition-duration 500ms, 500ms  
      -ms-transition-property width, height  
      -ms-transition-duration 500ms, 500ms
      -o-transition-property width, height  
      -o-transition-duration 500ms, 500ms
      transition-property width, height  
      transition-duration 500ms, 500ms

/* design panel */

#design-panel,
.dp-mask
  position absolute
  overflow hidden
  z-index 1
  opacity(1)
  top 100px
  background black
  border solid 5px #adadad
  rounded-corners()
  box-shadow(2px 2px 5px 0px rgba(0,0,0, .4))
  width 260px
  &.start-offscreen
    left -9999px

.dp-mask
  border 0
  padding 0 5px
  opacity(.5)

// general styles for generic elements when used in the design panel,
// the dt-style color picku

.dt-styles
  a.disabled
    color #666
    cursor default
  label
    color inherit
    font-size inherit
    position static
    display block
    width auto
    height auto
    text-transform none
    color inherit
    padding 0
    margin 0 
  .form-input
    color #ccc
  input[type=text],
  input[type=email],
  input[type=password],
  input[type=url],
  input[type=number],
  textarea,
  .richtextarea,
  .rta-toolbar,
  .form-box,
  div.help
    box-shadow(none)
    background transparent
    color #fff
    margin 0
    border-color #666
  .scrollList
    font-size 12px
    background #000
    color #ccc
    li
      color inherit
    li.selected
      color #fff
  .form-box
    background-color #333
  div.select
    border none
    background #333333
    color #ccc
    height 30px
    line-height 30px
    &.sliding-select
      min-height 29px
      padding-bottom 1px
      height auto
      margin-bottom 20px
      background transparent
    .form-input      
      line-height 28px
      padding 2px 10px 0
    .label
      background url(dt-arrow-dropdown) 96.6% 55% no-repeat
      color #ccc
      height 28px
      font-size 13px
      line-height 25px
      padding-left 10px
    .label:active,
    .label:hover
      color #fff
    &.open
      .control
        .label
          background-image url(dt-arrow-dropdown-open)
    &.disabled
      opacity(.5)
      .control
        .label:active,
        .label:hover
          color #ccc
        .ie8 &
          background-color #191919
          border-color #333
          .label
            color #676767
  .click-trap
    margin-top 12px
  .slider
    margin-top 0
    &, &.show-labels
      height auto
    &.show-labels
      margin-bottom 20px
    label
      width auto
      display block
      position absolute
      color #fff
      font-size font-size-20
    label.text
      font-size font-size-11
    .positioning-container
      position relative
      label
        top 14px
      label.text
        top 22px
    .track
      border-color #666
      background-color #333
      box-shadow(none)
    .tick
      background #666
    .thumb:focus
      outline 0
    &.show-input
      .positioning-container
        margin-right 45px
      input
        position absolute
        bottom 19px
        right 0
        width 32px
        font-size font-size-12
        line-height 20px
        height 22px
        padding 2px 0 0
        text-align center

.dt-error p
  margin-bottom 10px
  &:last-child
    margin-bottom 0

.dt-content > div.select
  margin-bottom 10px

.dt-label
