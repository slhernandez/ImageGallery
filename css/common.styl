      border-color #a70000
  &[disabled]
    &,
    &:hover
      clear-gradient(#f3f3f3)
      border-color #e6e6e6
      color #ccc
      text-shadow none
      cursor default
  &.icon-text-btn
    position relative
    padding 0 10px 0 3px
    span.icon
