@import 'nib/reset'
@import 'nib/gradients'
@import 'nib/vendor'

/* Reset */
global-reset()
reset-html5()

/* Normalize monospace sizing
   en.wikipedia.org/wiki/MediaWiki_talkCommon.css/Archive_11#Teletype_style_fix_for_Chrome */
pre, code, kbd, samp
  font-family monospace, sans-serif

/* Variables */
karnak-family = Rockwell, Georgia, serif
light-family = 'Helvetica Neue', Helvetica, Arial, sans-serif
regular-family = 'Helvetica Neue', Helvetica, Arial, sans-serif
bold-family = 'Helvetica Neue', Helvetica, Arial, sans-serif
border-radius-size = 6px
error-color = #ff0f0f
standard-focus-outline = thin dotted #999

/* Font sizes */
font-size-8 = 66.6667%
font-size-9 = 75%
font-size-10 = 83%
font-size-11 = 91.7%
font-size-12 = 100%
font-size-13 = 108.33%
font-size-14 = 116.7%
font-size-16 = 133.3%
font-size-18 = 150%
font-size-20 = 166.7%
font-size-22 = 183.33%
font-size-24 = 200%
font-size-40 = 333.3%
font-size-50 = 416.67%

/* Colors */
base-text-color = #333333
lighter-text-color = #4d4d4d

/* Reusable UI images */
dropper-cursor = 'data:image/jpeg;base64,'
color-wheel = 'data:image/jpeg;base64,'
slider-thumb-fill-with-dots = 'data:image/png;base64,'
magnifying-glass-icon = 'data:image/png;base64,'
green-circled-check = 'data:image/png;base64,'
dark-close-x = 'data:image/png;base64,'
arrow-dropdown = 'data:image/png;base64,'
arrow-dropdown-disabled = 'data:image/png;base64,'
arrow-dropdown-white = 'data:image/png;base64,'
arrow-dropdown-blue = 'data:image/png;base64,'
// used in datepicker, includes hover state but not disabled state
left-right-arrow-sprites-for-dark-bg = 'data:image/png;base64,'
// 29x28, includes disabled state (29x28 starting at y=28)
arrows-move-item-up = 'data:image/png;base64,'
// 29x28, includes disabled state (29x28 starting at y=28)
arrows-move-item-down = 'data:image/png;base64,'
// 30x28, includes disabled state (30x28 starting at y=28) 
arrows-nest-item = 'data:image/png;base64,'
// 30x28, includes disabled state (30x28 starting at y=28) 
arrows-denest-item = 'data:image/png;base64,'
// 30x28, includes disabled state (30x28 starting at y=28) 
double-arrows-left = 'data:image/png;base64,'
// 30x28, includes disabled state (30x28 starting at y=28) 
double-arrows-right = 'data:image/png;base64,'
// arrows for collapsible controls
arrow-collapsible-open = 'data:image/png;base64,'
arrow-collapsible-closed = 'data:image/png;base64,'
// 28x28
trash-can = 'data:image/png;base64,'
trash-can-disabled = 'data:image/png;base64,'
// 29x30
double-arrows-up-white = 'data:image/png;base64,'
double-arrows-down-white = 'data:image/png;base64,'
// arrow like the greater-than/less-than symbols, used for breadcrumbs back-arrows and option lists in design panel
arrow-lt-gray = 'data:image/png;base64,'
arrow-gt-gray-large = 'data:image/png;base64,'
// 24x23 (richtextarea toolbar)
ul-icon = 'data:image/png;base64,'
ol-icon = 'data:image/png;base64,'
link-icon = 'data:image/png;base64,'
unlink-icon = 'data:image/png;base64,'
// 24x23 (richtextarea toolbar disabled)
ul-icon-disabled = 'data:image/png;base64,'
ol-icon-disabled = 'data:image/png;base64,'
link-icon-disabled = 'data:image/png;base64,'
unlink-icon-disabled = 'data:image/png;base64,'
// Disabled close X
disabled-close-x = 'data:image/png;base64,'
pdf_icon_26x26 = 'data:image/png;base64,'
circle_plus = 'data:image/png;base64,'
// Close for header
header-close-icon = 'data:image/png;base64,'
// Add icon
add-icon = 'data:image/png;base64,'
// Check mark for saved field
field-saved = 'data:image/png;base64,'
field-saved-wide = 'data:image/png;base64,'
// Exclamation point for erorr field
field-error = 'data:image/png;base64,'
field-error-wide = 'data:image/png;base64,'
pdf_icon_16x16 = 'data:image/png;base64,'
// current-site pencil
icon-pencil = 'data:image/png;base64,'

/* Fonts */
regular()
  font-face(regular-family, 0.52, normal)

karnak()
  font-face(karnak-family, 0.47, 500)

light()
  font-face(light-family, 0.53, 200)

bold()
  font-face(bold-family, 0.54, bold)

/* Mixins */
font-face(family, size-adjust, font-weight)
  font-family family
  font-size-adjust size-adjust
  font-weight font-weight
  font-style normal
  -webkit-font-smoothing antialiased
  -moz-font-smoothing antialiased
  font-smoothing antialiased

gradient-rounded-corners()
  .modern &
    rounded-corners(arguments)

rounded-corners()
  if length(arguments) > 0
    border-radius(arguments)
  else
    border-radius(border-radius-size)

gradient-fill(top-color, bottom-color)
  background bottom-color
  background linear-gradient(top, top-color, bottom-color)
  ie-gradient(top-color, bottom-color)

ltr-gradient(left-color, right-color)
  background left-color
  background linear-gradient(left, left-color, right-color)
  ie-ltr-gradient(left-color, right-color)

ie-gradient(top-color, bottom-color)
  .ie8 &,
  .ie9 &
    filter 'progid:DXImageTransform.Microsoft.Alpha(opacity=100) progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=%s, endColorstr=%s)' % (argb(top-color) argb(bottom-color))
    -ms-filter 'progid:DXImageTransform.Microsoft.Alpha(opacity=100) progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=%s, endColorstr=%s)' % (argb(top-color) argb(bottom-color))

ie-ltr-gradient(left-color, right-color)
  .ie8 &,
  .ie9 &
    filter 'progid:DXImageTransform.Microsoft.gradient(GradientType=1,startColorstr=%s, endColorstr=%s)' % (argb(left-color) argb(right-color))
    -ms-filter 'progid:DXImageTransform.Microsoft.gradient(GradientType=1,startColorstr=%s, endColorstr=%s)' % (argb(left-color) argb(right-color))

clear-gradient(color)
  background color
  .ie8 &,
  .ie9 &
    filter: none
    -ms-filter: none

single-line(size)
  font-size size
  line-height 100%

/* Global */
html
  overflow-y scroll

body
  regular()
  font-size 12px
  line-height 1.25
  *font-size small
  color base-text-color
  background #f6f9fc

::selection,
::moz-selection
  background-color #b4d3ff
  text-shadow none

::-moz-focus-inner
  border 0
  padding 0

:focus
  a&,
  a.link-button&
  input&,
  textarea&,
  div[tabindex]&,
  label&,
  button&
    outline standard-focus-outline // normalize focus to fix Chrome's insane focus style
  a&
    outline-offset 1px
  input[type="checkbox"]&,
  button&,
  input[type=submit]&,
  a.link-button&
    outline-offset 2px

a
  &,
  &:visited,
  &:hover
    color #369
    outline none
    outline-color #999
  &, 
  &:visited
    text-decoration none
  &:hover
    text-decoration underline
  &[disabled]
    &,
    &:hover
      color #ccc
      text-decoration none
      cursor default

h1
  single-line(font-size-40)

h2
  single-line(font-size-18)

h3
  font-size font-size-14

.hidden
  display none

/* Fonts */
.karnak
  karnak()

.light,
h1 
  light()

.bold,
h2,
h3
  bold()

h2.light
  single-line(font-size-22)

/* Containers */
.outer
  position relative
  min-width 680px
  padding 0 20px

.inner
  position relative
  margin 0 auto
  width 93.5%
  min-width 680px
  max-width 1085px

.container
  position relative
  background-color #fff
  border solid 1px #d2d2d2
  rounded-corners()
  .content
    padding 20px
    h2
      margin-bottom 10px
  &.tabbed
    rounded-corners(top left 0)
    .content.active
      display block

#main
  padding 30px 0
  .not-supported &
  .no-js &
    display none

#not-supported
  display none
  padding-top 30px
  padding-left 0
  padding-right 0
  text-align center
  margin auto 0
  h1
    margin-bottom 20px
  p
    font-size 133%
    margin-bottom 20px
  .ie
    display none
  .not-supported &
    display block
  .ie &
    .ie
      display inline
  .upgrade
    display none
    .not-supported &
      display block
  .js
    display none
    .no-js &
      display block
  .not-supported &,
  .no-js &
    display block

.fixed-sidebar-layout
  display table
  box-sizing(border-box)
  width 100%
  > div
    vertical-align top
    box-sizing(border-box)
    display table-cell
  > .fixed-sidebar
    vertical-align top
    box-sizing(border-box)
    display table-cell
    width 292px
    padding-left 28px
  .container & h2
    margin-bottom 20px
  .wrapper
    padding 20px 20px 0 20px
  .title
    background-color #f3f3f3
    border-bottom solid 1px #ccc;
    rounded-corners(border-radius-size border-radius-size 0 0)
    h2,
    h3
      margin-bottom 20px
  aside
    .container
      margin-bottom 30px

/* Utility Nav */
.hanging-tabs
  overflow hidden
  bold()
  ul
    list-style none
  li
    float left
    margin-left 20px
  li a
    display block
    height 30px
    line-height 30px
    padding 0 20px
    color #fff
    background base-text-color
    text-shadow 1px 1px 1px #000
    border solid 1px transparent
    border-top-width 0
    rounded-corners(bottom border-radius-size)
    &:hover
      background #000
      text-decoration none
    &:focus
      outline 0
      border-color #555

utility-active()
  background #000
  text-decoration none

#utility
  position absolute
  right -7px
  top 7px
  ul
    list-style none
  li
    position relative
    display inline-block
    cursor pointer
    .icon
      display inline-block
      width 38px
      height 48px
      overflow hidden
      white-space no-wrap
      text-indent 100%
      background-position 50% 0px
      background-repeat no-repeat
      border-top solid 1px transparent
      &:hover
        background-position 50% -50px
      &[disabled]
        &,
        &:hover,
        &:active
          background-position 50% -98px
      &.selected a
        &,
        &:hover
          background-position 50% 0px
      &.settings
        background-image url('data:image/png;base64,')
      &.support
        background-image url('data:image/png;base64,')
      &.account
        background-image url('data:image/png;base64,')
      &.open
        width 36px
        background-position 50% -50px
        background-color #fff
        border solid 1px #ccc
        border-bottom none
        rounded-corners(6px 6px 0 0)

#account-menu
  position absolute
  top 48px
  right -7px
  width 224px
  background #fff
  border solid 1px #ccc
  rounded-corners(6px 0 6px 6px)
  z-index 1000
  overflow hidden
  display none
  &:after
    content ''
    position absolute
    top -1px
    right 0
    width 44px
    height 1px
    display block
    background #fff
  ul
    padding-top 20px
  ul.site-list
    padding 10px 0
    li
      padding 8px 15px
  li
    font-size font-size-14
    padding 0 15px 15px 15px
    color #4d4d4d
    &.divider
      border-bottom solid 1px #ccc
    &.current-site
      background #f6ffed
      span
        font-weight bold
        display block
        font-style normal
        background url(icon-pencil) right center no-repeat
        padding-right 20px
  a.add-site
    display block
    font-size font-size-14
    background url('data:image/png;base64,') left center no-repeat
    padding 15px 0 15px 26px
    margin-left 15px

/* Global Header */
#global
  position relative
  height 57px
  padding 0
  h1
    position absolute
    top 18px
    &.logo,
    a
      display block
      width 143px
      height 25px
      text-indent 100%
      overflow hidden
      white-space nowrap
      background url('data:image/png;base64,') 0 0 no-repeat 
      /* ' vim, give me back my formatting */

/* Close buttons */
.close
  position absolute
  top 20px
  right 20px
  width 10px
  height 10px
  text-indent 100%
  background url('data:image/png;base64,') 0 0 no-repeat
  overflow hidden
  white-space nowrap
  &.dark
    background-image url(dark-close-x)
  &.dark[disabled]
      background-image url(disabled-close-x)
  &.button
    width 20px
    height 19px
    background-image url('data:image/png;base64,')


/* Header */
#header
  position relative
  height 57px
  .outer
    background #333
  div.arrow
    position absolute
    top 57px
    width 12px
    height 8px
    z-index 1
    background url('data:image/png;base64,')


/* Tabs */
active-tab()
  color base-text-color
  text-shadow none
  clear-gradient(#fff)
  border-color #d2d2d2
  border-bottom solid 1px #fff

nav.tabs
  font-size font-size-13
  bold()
  ol
    position relative
    margin-bottom -1px
    z-index 100
    list-style none
  li
    list-style none
    display inline-block
    margin-right 6px
  a
    color lighter-text-color
    text-transform uppercase
    text-shadow 1px 1px 1px #fff
    gradient-fill(#fbfbfb, #d9d9d9)
    display inline-block
    line-height 48px
    padding 0 20px
    border solid 1px #ccc
    border-bottom none
    gradient-rounded-corners(top border-radius-size)
    &:hover
      text-decoration none
      gradient-fill(#ededed, #ccc)
    &[disabled]
      &,
      &:hover
        color #ccc
        clear-gradient(#f3f3f3)
        text-shadow none
        cursor default
    &.active
      &,
      &:hover
        active-tab()

/* Errors */
ul.errors
  font-size font-size-14
  color error-color
  bold()
  margin-bottom 10px
