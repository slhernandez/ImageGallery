    background url('data:image/png;base64,') 0 0 no-repeat
    padding-left 32px
    line-height 21px
    margin-top 10px
  fieldset > & > li:first-child {
    margin-top: 0px
  }

/* Form */
button,
input,
select,
a.link-button,
textarea,
.form-input
  regular()

.ie8, .ie9
  input[type=checkbox],
  input[type=radio]
    overflow hidden 

button,
input[type=submit],
a.link-button
  color lighter-text-color
  text-align center
  border none
  gradient-rounded-corners()
  padding 0 20px
  cursor pointer
  text-shadow 1px 1px 1px #fff
  gradient-fill(#fbfbfb, #d9d9d9)
  border solid 1px #ccc
  font-size font-size-11
  line-height 28px
  box-sizing(border-box)
  height 30px
  display inline-block
  bold()
  &.large
    font-size font-size-14
    line-height 34px
    height 37px
  &:hover
    text-decoration none
    gradient-fill(#ededed, #ccc)
    border-color #bfbfbf
  &.accent
    color #fff
    text-shadow 1px 1px 1px #3a7110
    gradient-fill(#61bf1a, #55a617)
    border-color #4e9915
